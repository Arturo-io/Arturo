        <tr class="build" id="<%= build.id %>">
                <td class="name">
                  <%= link_to "#{build.repo.full_name} ##{build.id}", url_for(controller: 'build', action: 'show', id: build[:id], only_path: true) %>
                </td>
              <td class="branch"><%= build.branch %></td>
              <td class="commit">
                  <%= link_to build.commit, build.commit_url, target: '_blank' %>
              </td>

              <td class="author"><%= build.author %></td>
              <td class="status <%= build.status.downcase%>">
                <%= build.status %>
              </td>
            </tr>

