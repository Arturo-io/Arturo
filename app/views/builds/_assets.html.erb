        <div class="row">
          <div class="small-12 columns">
            <table class="assets">
              <thead>
                <tr>
                  <th>File Name</th>
                  <th>Created</th>
                </tr>
              </thead>
              <tbody>
              <% assets.each do |asset| %> 
                  <tr class="asset">
                    <td class="url">
                      <%= link_to File.basename(asset.url), asset.url %>
                    </td>
                    <td class="created">
                      <%= timeago_tag asset.created_at %>
                    </td>
                </tr>
              <% end %>
              </tbody>
            </table>
          </div>
        </div>

